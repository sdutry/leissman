<!DOCTYPE html>
<html>
  <head>
    <title>{{title}}</title>

    <link rel="stylesheet" href="/assets/css/main.min.css">

    <script src="/assets/js/main.min.js"></script>
  </head>
  <body id="{{id}}">
    {{> navbar}}
    <div class="container">
      {{#if jumbotitle}}
      <div class="jumbotron">
        <h1>{{jumbotitle}}</h1>
      </div>
      {{/if}}
      {{#if banner}}
        <img src="{{banner}}" class="img-responsive">
      {{/if}}
      {{>body}}
    </div>
  </body>
</html>

