---
id: stardew
title: Stardew
banner: /assets/images/stardew/banner.png
---
<h2>Farms</h2>
<div class="row">
  {{#each stardew.farms}}
    <div class="col-md-6">
      <div class="panel panel-primary text-center clearfix">
        <div class="panel-heading"><h4>{{./name}}</h4></div>
        <div class="panel-body">
          <img src="{{./image}}" class="center-block img-responsive overlay">
        </div>
        <div class="panel-footer">{{./description}}</div>
      </div>
    </div>
  {{/each}}
</div>
<div class="row">
  <div class="modal fade" id="imagemodal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">              
        <div class="modal-body">
      	  <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
          <img src="" id="modalOverlayImage" class="img-responsive">
        </div>
      </div>
    </div>
  </div>
</div>

